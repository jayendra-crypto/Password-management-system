          <th scope="col">password-category name</th>

          <th scope="col">Action</th>

        </tr>
      </thead>
      <tbody style="background-color:rgb(215, 238, 245);">
        <% if(records.length>0){
          records.forEach(function(row){  %>

        <tr>


          <td><%=row.password_category %></td>

          <td><a class="btn btn-primary btn-xs" href="/password-category/edit/<%=row._id %>"><i class="fa fa-edit"></i></a>
            <a class="btn btn-danger btn-xs" href="/password-category/delete/<%=row._id %>"><i class="fa fa-trash"></i></a></td>

        </tr>
        <% })} %>



      </tbody>

    </table>
